{
  "name": "planyourcourses.com",
  "version": "0.0.1",
  "description": "Plan your courses",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch --verbose",
    "test:coverage": "jest --verbose --coverage",
    "typecheck": "flow check src",
    "lint": "eslint ./src",
    "build": "npm run lint && npm run typecheck && npm run test && webpack --progress -p",
    "start": "NODE_ENV=prod npm run build && http-server",
    "devServer": "NODE_ENV=dev node server.js"
  },
  "keywords": [
    "React",
    "Mobx",
    "Aphrodite",
    "Jest",
    "Flow"
  ],
  "author": "Wade Mauger",
  "dependencies": {
    "aphrodite": "^0.3.1",
    "babel-register": "^6.8.0",
    "css-loader": "^0.24.0",
    "empty": "^0.10.1",
    "http-server": "^0.9.0",
    "mobx": "^3.4.1",
    "mobx-react": "^4.3.5",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "reactstrap": "^5.0.0-alpha.4",
    "resolve-url-loader": "^1.6.0",
    "style-loader": "^0.13.1"
  },
  "devDependencies": {
    "babel-core": "^6.8.0",
    "babel-eslint": "^6.0.4",
    "babel-jest": "^12.0.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-array-includes": "^2.0.3",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-plugin-transform-react-constant-elements": "^6.8.0",
    "babel-plugin-transform-react-display-name": "^6.8.0",
    "babel-plugin-transform-react-inline-elements": "^6.8.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.6",
    "babel-plugin-transform-runtime": "^6.8.0",
    "babel-plugin-typecheck": "^3.8.0",
    "babel-polyfill": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-es2015-loose": "^7.0.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "babel-register": "^6.8.0",
    "babel-types": "^6.8.1",
